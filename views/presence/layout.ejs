<!DOCTYPE html>
<html ng-app="PresenceApplication">

<head>
    <title>
        <%=typeof title == 'undefined' ? 'New Sails App' : title%>
    </title>

    <!-- Viewport mobile tag for sensible mobile support -->
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Angular Material style sheet -->
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/angular_material/1.1.0/angular-material.min.css">

    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400italic,600' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="/styles/angular-clock.css">
    <link rel="stylesheet" href="/styles/angular-toastr.min.css">
    <link rel="stylesheet" href="/styles/main.css">

</head>

<body layout="row" class="icondemoFontIconsWithLigatures">
    <%- body %>
        <script src="/js/dependencies/lodash.min.js"></script>
        <script src="/js/dependencies/sails.io.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular.min.js">
        </script>
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular-animate.min.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular-aria.min.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.5.5/angular-messages.min.js"></script>

        <!-- Angular Material Library -->
        <script src="https://ajax.googleapis.com/ajax/libs/angular_material/1.1.0/angular-material.min.js">
        </script>

        <script src="/js/dependencies/angular-clock.min.js"></script>
        <script src="/js/dependencies/sailsResource.js"></script>
        <script src="/js/dependencies/angular-toastr.min.js"></script>
        <script src="/js/dependencies/angular-toastr.tpls.min.js"></script>
        <script src="/js/dependencies/angular-ui-router.min.js"></script>

        <script src="/js/presence.app.js"></script>
</body>

</html>